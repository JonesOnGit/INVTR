<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Ads Served</th>
      <th>Mobile</th>
      <th>Desktop</th>
      <th>Ads Clicked</th>
    </tr>
  </thead>
  <tbody>
  <% stats.each do |stat| %>

	  <tr>
	    <td><%= stat[0] %></td>
	    <td><%= stat[1]["count"] %></td>
      <td><%= stat[1]["mobile"] %></td>
      <td><%= stat[1]["desktop"] %></td>
	  </tr>
  <% end %>
  </tbody>
</table> 
